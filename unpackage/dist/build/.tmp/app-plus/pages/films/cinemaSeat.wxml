<view><view class="cu-list menu"><view class="cu-item"><view class="content padding-tb-sm"><view>{{film.filmName}}</view><view class="text-gray text-sm">{{currentDate+" "+session.playTime+"~"+session.endTime+"("+film.dimensional+")"}}</view></view></view><view class="cu-item"><view class="content text-center"><view class="text-gray text-sm text-xxl"><image style="width:30px;height:30px;" src="../../static/noseat.png"></image><text class="text-sm" style="margin-top:-10px;">可选</text><image style="width:30px;height:30px;" src="../../static/otherseat.png"></image><text class="text-sm">已售</text><image style="width:30px;height:30px;" src="../../static/myseat.png"></image><text class="text-sm">自己</text></view></view></view></view><view><view class="grid col-3 margin-left-sm"><view class="bg-gray"></view><view class="bg-grey text-center" style="border-radius:5px;"><text class="text-sm">{{hall.hallName+"屏幕"}}</text></view><view class="bg-gray"></view></view><view class="text-sl text-center padding"><t-table class="bg-white" vue-id="1" border="0" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{hall.row}}" wx:for-item="index" wx:for-index="__i0__" wx:key="*this"><t-tr class="bg-gray" vue-id="{{('2-'+__i0__)+','+('1')}}" bind:__l="__l" vue-slots="{{['default']}}"><text class="text-gray text-sm">{{index+1}}</text><block wx:for="{{hall.column}}" wx:for-item="index1" wx:for-index="__i1__" wx:key="*this"><t-td vue-id="{{('3-'+__i0__+'-'+__i1__)+','+('2-'+__i0__)}}" bind:__l="__l" vue-slots="{{['default']}}"><image style="width:35px;height:35px;" src="{{seatInfo[index][index1]}}" data-event-opts="{{[['tap',[['chooseSeat',[[index+1,index1+1]]]]]]}}" bindtap="__e"></image></t-td></block><view class="_br"></view></t-tr></block></t-table></view></view><block wx:if="{{seats.length!==0}}"><view class="cu-item bg-white"><view class="content"><view class="grid col-6 grid-square margin-left-sm"><block wx:for="{{seats}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="margin-top bg-orange align-center"><view><text>{{item.row+"排"+item.coloum+"座"}}</text></view></view></block></view></view></view></block><view class="button padding flex flex-direction"><block wx:if="{{seats.length!==0}}"><button data-event-opts="{{[['tap',[['confirmSeat',['$event']]]]]}}" class="cu-btn bg-red margin-tb-sm lg" bindtap="__e">{{money+"确认选座"}}</button></block><block wx:else><button class="cu-btn bg-red margin-tb-sm lg" disabled>请先选座</button></block></view></view>